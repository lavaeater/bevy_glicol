{
  "keybindings": {
    "Home": {
      "<q>": "Quit", // Quit the application
      "<Ctrl-d>": "Quit", // Another way to quit
      "<Ctrl-c>": "Quit", // Yet another way to quit
      "<Ctrl-z>": "Suspend", // Suspend the application
      "<p>": "PlayAudio", // Start playing audio
      "<t>": "SpecialAudio", // SPECIAL
      "<s>": "StopAudio", // Stop playing audio
      "<u>": { "UpdateAudioCode": "out: saw 440.0 >> mul 0.1" } // Update audio code (example)
    }
  }
}
